{
	"schemaVersion": 1,
	"id": "learnfabric",
	"version": "${version}",
	"name": "LearnFabric",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Pyrotonic"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/learnfabric/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "net.pyrotonic.learnfabric.LearnFabric"
		],
		"fabric-datagen": [
			"net.pyrotonic.learnfabric.LearnFabricDataGenerator"
		]
	},
	"mixins": [
		"learnfabric.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}